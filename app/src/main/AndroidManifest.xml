<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.laptopshopapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".titlePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Computers"></activity>
        <activity android:name=".UserPage"></activity>
        <activity android:name=".SignIn"></activity>
        <activity android:name=".singleComp"></activity>
        <activity android:name=".Buy"></activity>
        <activity android:name=".Confirmed_Order"></activity>
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name=".Confirmed_register"></activity>
        <activity android:name=".Account_Info"></activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>